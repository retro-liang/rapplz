	</app-search>
	
	<app-feed>
		<ios>
			<all>
				<new>http://itunes.apple.com/us/rss/newapplications/limit=5/xml</new>
				<top25-paid>http://itunes.apple.com/us/rss/newapplications/limit=5/xml</top25-paid>			
			</all>
			<iphone>
				<top25-paid>http://itunes.apple.com/us/rss/newapplications/limit=5/xml</top25-paid>
			</iphone>
		</ios>
	</app-feed>
