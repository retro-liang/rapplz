<?xml version="1.0" encoding="UTF-8" ?>
<rapplz>
	<security-key>retro</security-key>
	<sender-email-address>contact@rapplz.com</sender-email-address>
	<app-search>
		<ios>
			<api>http://itunes.apple.com/search?</api>
			<parameters>
				<country>us</country>
				<entity>software</entity>
				<attribute>softwareDeveloper</attribute>
				<limit>10</limit>
			</parameters>			
